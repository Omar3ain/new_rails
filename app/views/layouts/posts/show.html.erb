<div id="<%= dom_id(@post) %>">
  <p>
    <strong>Title:</strong> <%= @post.title %>
  </p>
  <p>
    <strong>Content:</strong> <%= @post.content %>
  </p>
</div>

<%= link_to 'Edit', edit_post_path(@post) %> 
<%= link_to 'Back', posts_path %> 
<%= button_to 'Destroy this post', @post, method: :delete %>