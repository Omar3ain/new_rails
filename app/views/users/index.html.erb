<h1>Users#index</h1>

<p style="color: green"><%= notice %></p>

<h1>User</h1>

<div id="users">
  <% @users.each do |user| %>
    <p>
        id : <%= user.id %> <br>
        Name: <%= user.name %>
    </p>

     <p>
        Email: <%= user.email %>
    </p>

    <p>
      <%= link_to "Show this User", user %>
    </p>
  <% end %>
</div>

<%= link_to "New User", new_user_path %>

