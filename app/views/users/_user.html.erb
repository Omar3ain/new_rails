<div id="<%= dom_id user %>">
    <p>
        <strong>Name:</strong>
         <%= user.name %>
    </p>
    <p>
        <strong>Date Of Birth:</strong>
         <%= user.DOB %>
    </p>
    <p>
        <strong>Email:</strong>
         <%= user.email %>
    </p>
    <p>
        <strong>Phone Number:</strong>
         <%= user.phone_number %>
    </p>
</div>