
<h1>Post <%=@post[:title] %></h1>

<div id="posts">
  
    <%= render @post %>
    <p>
      <%= link_to "Edit this post", "#{post_path(@post)}/edit" %>
      <%= link_to "Back to posts", posts_path %>
      <%= link_to "Destroy this post", "#{post_path(@post)}", method: :delete %>
    </p>
 
</div>

